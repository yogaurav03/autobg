{
  "name": "react-native-image-resizer",
  "version": "3.0.11",
  "summary": "Rescale local images with React Native",
  "homepage": "https://github.com/bamlab/react-native-image-resizer#readme",
  "license": "MIT",
  "authors": "Florian Rival <florianr@bam.tech> (http://bam.tech)",
  "platforms": {
    "ios": "10.0"
  },
  "source": {
    "git": "https://github.com/taboulot/react-native-image-resizer.git",
    "tag": "3.0.11"
  },
  "source_files": "ios/**/*.{h,m,mm}",
  "ios": {
    "frameworks": [
      "AssetsLibrary",
      "MobileCoreServices"
    ]
  },
  "compiler_flags": "-DFOLLY_NO_CONFIG -DFOLLY_MOBILE=1 -DFOLLY_USE_LIBCPP=1 -Wno-comma -Wno-shorten-64-to-32 -DRCT_NEW_ARCH_ENABLED=1  -DRCT_NEW_ARCH_ENABLED=1 -DFOLLY_NO_CONFIG -DFOLLY_MOBILE=1 -DFOLLY_USE_LIBCPP=1 -DFOLLY_CFG_NO_COROUTINES=1 -DFOLLY_HAVE_CLOCK_GETTIME=1 -Wno-comma -Wno-shorten-64-to-32",
  "pod_target_xcconfig": {
    "HEADER_SEARCH_PATHS": "\"$(PODS_ROOT)/boost\" \"$(PODS_ROOT)/boost\" \"$(PODS_ROOT)/Headers/Private/Yoga\"",
    "OTHER_CPLUSPLUSFLAGS": "$(inherited) -DFOLLY_NO_CONFIG -DFOLLY_MOBILE=1 -DFOLLY_USE_LIBCPP=1 -DRCT_NEW_ARCH_ENABLED=1 -DFOLLY_NO_CONFIG -DFOLLY_MOBILE=1 -DFOLLY_USE_LIBCPP=1 -DFOLLY_CFG_NO_COROUTINES=1 -DFOLLY_HAVE_CLOCK_GETTIME=1 -Wno-comma -Wno-shorten-64-to-32",
    "CLANG_CXX_LANGUAGE_STANDARD": "c++20"
  },
  "dependencies": {
    "React-Codegen": [],
    "RCT-Folly": [
      "2024.01.01.00"
    ],
    "RCTRequired": [],
    "RCTTypeSafety": [],
    "ReactCommon/turbomodule/core": [],
    "React-Core": [],
    "glog": [],
    "React-RCTFabric": [],
    "ReactCodegen": [],
    "ReactCommon/turbomodule/bridging": [],
    "React-NativeModulesApple": [],
    "Yoga": [],
    "React-Fabric": [],
    "React-graphics": [],
    "React-utils": [],
    "React-featureflags": [],
    "React-debug": [],
    "React-ImageManager": [],
    "React-rendererdebug": [],
    "DoubleConversion": [],
    "hermes-engine": []
  }
}
